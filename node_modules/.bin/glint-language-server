#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/core/bin/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/core/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/core/bin/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/core/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules/@glint/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@glint+core@1.0.2_typescript@5.0.4/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@glint/core/bin/glint-language-server.js" "$@"
else
  exec node  "$basedir/../@glint/core/bin/glint-language-server.js" "$@"
fi
