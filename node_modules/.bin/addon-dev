#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/addon-dev/src/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/addon-dev/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/addon-dev/src/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/addon-dev/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules/@embroider/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/@embroider+addon-dev@3.0.0_rollup@3.21.8/node_modules:/home/nullvoxpopuli/Development/tmp/no-rollup-plugin-ts-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@embroider/addon-dev/src/commands.js" "$@"
else
  exec node  "$basedir/../@embroider/addon-dev/src/commands.js" "$@"
fi
